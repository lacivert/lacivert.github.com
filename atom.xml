<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Karl Seguin</title>
 <link href="http://lacivert.github.com/atom.xml" rel="self"/>
 <link href="http://lacivert.github.com/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://lacivert.github.com/</id>
 <author>
   <name>lacivert</name>
   <email>yasin@mafya.com</email>
 </author>

 {% for post in site.posts limit:10 %}
 <entry>
   <title>{{ post.title | xml_escape }}</title>
   <link href="http://lacivert.github.com{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://lacivert.github.com{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>